<h2>Admins</h2>
<% @admins.each do |admin| %>
  <a href="#" class="dropdown-toggle dropdown icon-edit" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-edit icon"></i></a>
  <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
    <a href="#userEditModal<%= admin.id %>" data-toggle="modal" data-target="#userEditModal<%= admin.id %>" class="dropdown-item dropdown-item-top" data-edit=" <%= @user = admin %>">Edit</a>
    <%= link_to "Destroy", users_path, method: :delete, data: { confirm: 'Are you sure?' }, class: "dropdown-item dropdown-item-bottom" %>
  </div>
  <div class="user">
    <%= admin.first_name %>
    <%= admin.last_name %>
    <%= admin.email %>
    <%= admin.phone_number %>
  </div>
<% end %>
