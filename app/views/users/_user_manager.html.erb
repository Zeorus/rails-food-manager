<h2>Managers</h2>
<% @managers.each do |manager| %>
  <a href="#" class="dropdown-toggle dropdown icon-edit" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-edit icon"></i></a>
  <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
    <a href="#productEditModal<%= manager.id %>" data-toggle="modal" data-target="#userEditModal<%= manager.id %>" class="dropdown-item dropdown-item-top" data-edit=" <%= @user = manager %>">Edit</a>
    <%= link_to "Destroy", users_path, method: :delete, data: { confirm: 'Are you sure?' }, class: "dropdown-item dropdown-item-bottom" %>
  </div>
  <div class="user">
    <%= manager.first_name %>
    <%= manager.last_name %>
    <%= manager.email %>
    <%= manager.phone_number %>
  </div>
<% end %>
